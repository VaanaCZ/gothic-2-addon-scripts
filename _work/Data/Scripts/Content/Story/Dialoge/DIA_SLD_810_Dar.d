};

func int DIA_Dar_ORCRING_Condition ()
{
	if (Kapitel >= 4)
		{
				return TRUE;
		};
};

func void DIA_Dar_ORCRING_Info ()
{
	AI_Output			(other, self, "DIA_Dar_ORCRING_15_00"); //Parece que faltan mercenarios.
	AI_Output			(self, other, "DIA_Dar_ORCRING_03_01"); //Sí, es verdad. Sylvio está a varios kilómetros de aquí y se llevó con él a la mitad de los hombres.
	AI_Output			(self, other, "DIA_Dar_ORCRING_03_02"); //¿A quién le importa? Tengo más posibilidades de convertirme en alguien junto con los chicos de Lee. Solo necesito es un golpe de efecto.
	AI_Output			(self, other, "DIA_Dar_ORCRING_03_03"); //Si puedo demostrar que soy un tipo duro, podría llegar a convertirme en uno de los guardaespaldas de Lee.

	Info_ClearChoices	(DIA_Dar_ORCRING);
	Info_AddChoice	(DIA_Dar_ORCRING, "No es asunto mío.", DIA_Dar_ORCRING_no );
	Info_AddChoice	(DIA_Dar_ORCRING, "¿Un tipo duro? ¿Tú?", DIA_Dar_ORCRING_necken );
	
	if ((hero.guild == GIL_SLD) || (hero.guild == GIL_DJG) || (hero.guild == GIL_KDF))
	{
		Info_AddChoice	(DIA_Dar_ORCRING, "¿Como qué?", DIA_Dar_ORCRING_wie );
	};
};

func void DIA_Dar_ORCRING_necken ()
{
	AI_Output			(other, self, "DIA_Dar_ORCRING_necken_15_00"); //¿Un tipo duro? ¿Tú?
	AI_Output			(self, other, "DIA_Dar_ORCRING_necken_03_01"); //(Enfadado) Oh, cállate, vamos. De todas maneras, ¿quién eres tú?
	
		if ((hero.guild == GIL_MIL) || (hero.guild == GIL_PAL))
		{
			AI_Output			(self, other, "DIA_Dar_ORCRING_necken_03_02"); //Un cretino de ciudad venido a más. Los tontos como tú no sois capaces de hacer nada bien.
		};

		if ((hero.guild == GIL_SLD) || (hero.guild == GIL_DJG))
		{
			AI_Output			(self, other, "DIA_Dar_ORCRING_necken_03_03"); //Solo llevas aquí un par de días y ya estás rajando como un loco.
		};

		if (hero.guild == GIL_KDF)
		{
			AI_Output			(self, other, "DIA_Dar_ORCRING_necken_03_04"); //¿A quién intentas asustar con tus estúpidas mierdas mágicas? Desde luego, a mí no.
		}; 
	

	if ((hero.guild == GIL_MIL) || (hero.guild == GIL_PAL))
	{
		AI_Output			(self, other, "DIA_Dar_ORCRING_necken_03_05"); //Ahora que lo pienso, aplastar tu cabeza es exactamente lo que haría que los chicos de Lee me respetaran.

		Info_ClearChoices	(DIA_Dar_ORCRING);
		Info_AddChoice	(DIA_Dar_ORCRING, "No tengo tiempo para este tipo de tonterías.", DIA_Dar_ORCRING_necken_no );
		Info_AddChoice	(DIA_Dar_ORCRING, "De acuerdo. Inténtalo.", DIA_Dar_ORCRING_necken_schlagen );
	}
	else
	{
		AI_Output			(self, other, "DIA_Dar_ORCRING_necken_03_06"); //Espera. Encontraré la forma de impresionar a Lee.
	};
};
var int Dar_FightAgainstPaladin;
